{
  "name": "g2i-node-challenge",
  "version": "1.0.0",
  "description": "G2i Node Challenge",
  "main": "index.ts",
  "author": "Moses Idowu",
  "license": "MIT",
  "private": true,
  "keywords": [],
  "scripts": {
    "start": "npx dotenv -e .env.local ts-node index.ts",
    "build": "tsc -p .",
    "dev": "nodemon -r dotenv/config --exec ts-node index.ts dotenv_config_path=.env.local",
    "seed-data": "npx dotenv -e .env.local -- ts-node-dev scripts/seed-data.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "docker-prod-start": "ts-node index.ts",
    "docker-start": "docker-compose up",
    "docker-build": "docker-compose build",
    "docker-stop": "docker-compose down",
    "docker-clean": "docker system prune -af",
    "docker-clean-volumes": "docker volume prune -f",
    "docker:db:console": "docker-compose exec postgres psql localhost -U postgres -d g2i",
    "docker:db:migrate-up": "docker-compose run web npx db-migrate up",
    "docker:db:migrate-down": "docker-compose run web npx db-migrate down",
    "docker:db:generate": "docker-compose run web npx prisma generate",
    "docker:rebuild": "docker-compose build web && docker-compose rm -rf web && docker-compose up -d",
    "docker:seed-data": "docker-compose run web npx ts-node scripts/seed-data.ts",
    "lint": "eslint --ext .ts --ext .js . --fix",
    "migrate:up": "yarn db-migrate up",
    "migrate:down": "yarn db-migrate down",
    "migrate:save": "npx dotenv -e .env.local -- yarn db-migrate create",
    "introspect": "npx dotenv -e .env.local -- yarn prisma db pull --force",
    "studio": "npx dotenv -e .env.local -- yarn prisma studio",
    "migrate:local": "npx dotenv -e .env.local -- yarn migrate:up",
    "migrate:local:down": "npx dotenv -e .env.local -- yarn migrate:down",
    "migrate:test": "npx dotenv -e .env.test.local -- yarn migrate:up",
    "migrate:test:down": "npx dotenv -e .env.test.local -- yarn migrate:down",
    "migrate": "yarn migrate:local && yarn migrate:test",
    "migrate-down": "yarn migrate:local:down && yarn migrate:test:down",
    "prepdb": "yarn migrate && yarn introspect && yarn prisma generate",
    "prepdb:down": "yarn migrate-down && yarn introspect && yarn prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@types/ioredis": "^4.28.10",
    "@types/ioredis-mock": "^5.6.0",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "ts-node": "^10.7.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "ioredis-mock": "^7.2.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "post-merge": "yarn"
    }
  }
}
